{
  "name": "usp/moodle",
  "repositories":{
    "clamp": {
      "type":"composer",
      "url":"https://moodlegist.clamp-it.org"
    }
  },
  "require": {
    "moodle/moodle": "3.9.2",
    "moodle-plugin-db/mod_attendance":"2020082500",
    "moodle-plugin-db/block_analytics_graphs": "*"
  },
  "extra": {
    "installerdir": "moodle",
    "installer-paths": {
      "moodle/mod/{$name}": ["type:moodle-mod"],
      "moodle/auth/{$name}": ["type:moodle-auth"],
      "moodle/block/{$name}": ["type:moodle-block"]
    }
  },
  "autoload": {
    "psr-4": {
      "MoodleComposer\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": "MoodleComposer\\MoodleComposer::postInstall"
  }
}
